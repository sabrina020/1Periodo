programa
{
	
	funcao inicio()
	{
		inteiro cont=1,soma=0
		enquanto(cont<=100){
			escreva(cont,"\n")
			soma=soma+cont
			cont++//ou cont=cont+1
		}
		escreva("soma dos valores: ",soma)
	}
}
